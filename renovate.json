{
    "rangeStrategy": "replace",
    "ignorePaths": [
        "lib/vscode/extensions",
        "lib/vscode/remote",
        "lib/vscode/test",
        "lib/vscode/.devcontainer",
        "lib/vscode/build/azure-pipelines"
    ],
    "extends": [
        "config:base"
    ],
    "packageRules": [
        {
            "matchUpdateTypes": ["minor", "patch", "digest"],
            "automerge": true
        }
    ]
}
